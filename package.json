{
  "name": "api-solid-gym",
  "version": "1.0.0",
  "description": "Gympass style app",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "test": "vitest run --project unit",
    "test:e2e": "vitest run --project e2e",
    "test:watch": "vitest --project unit",
    "test:e2e:watch": "vitest --project e2e",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fernandosantoshs/solid-gym-api.git"
  },
  "keywords": [
    "API",
    "REST",
    "SOLID"
  ],
  "author": "Fernando Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fernandosantoshs/solid-gym-api/issues"
  },
  "homepage": "https://github.com/fernandosantoshs/solid-gym-api#readme",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/node": "^22.8.1",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.1.2",
    "@vitest/ui": "3.1.2",
    "eslint": "9.13.0",
    "prisma": "5.21.1",
    "supertest": "7.1.0",
    "tsup": "8.4.0",
    "tsx": "4.19.4",
    "typescript": "5.6.3",
    "vite": "6.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.2"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.1.0",
    "@prisma/client": "5.21.1",
    "@types/bcrypt": "5.0.2",
    "bcrypt": "5.1.1",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "fastify": "5.3.2",
    "zod": "3.23.8"
  }
}
