  "bugs": {
    "url": "https://github.com/fernandosantoshs/solid-gym-api/issues"
  },
  "homepage": "https://github.com/fernandosantoshs/solid-gym-api#readme",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/node": "^22.8.1",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "eslint": "9.13.0",
    "prisma": "5.21.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8"
  },
  "dependencies": {
    "@prisma/client": "5.21.1",
    "@types/bcrypt": "5.0.2",
